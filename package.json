{
  "name": "deepseek-chat-project",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "client": "npm start --workspace=client",
    "server": "npm run dev --workspace=server",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "npm run build --workspace=client",
    "deploy": "npm run build && gh-pages -d client/build"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "gh-pages": "^6.1.0"
  },
  "homepage": "https://YAN-MQ.github.io/deepseek-chat"
} 